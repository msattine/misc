alias c clear
alias owd "echo $owd"
#Creating kinda global variable to use across opened shells
alias ccmd 'xargs echo > ~/.myvar'
alias pcmd 'set cmd = `cat ~/.myvar`;'
set filec
set nobeep
set autolist=ambiguous
bindkey "^w" backward-delete-word
bindkey "^d" history-search-forward
bindkey "^s" history-search-backward
set complete=enhance
alias gvt "gvim --remote-tab"
